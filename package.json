{
  "name": "biz-template",
  "version": "0.0.1",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/",
    "index.js",
    "README.md",
    "CHANGELOG.md",
    "package.json"
  ],
  "scripts": {
    "clean": "rimraf --no-glob ./dist ./coverage ./tsconfig.tsbuildinfo",
    "lint": "eslint . --ext .ts,.mts,.tsx,.js,.jsx,.cjs,.mjs --cache --cache-location .cache/eslint/main.eslintcache",
    "test": "run-s test-unit",
    "test-unit": "vitest run --passWithNoTests",
    "test-unit-watch": "vitest watch --ui",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "lint-staged-files": "lint-staged --allow-empty",
    "build": "npx flat build dynamic --publish-now --pack-now --timestamp=dynamic --clean-node-modules",
    "parallel": "npx flat mount parallel --prefix abc"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "@dimjs/lang": "^1.2.44",
    "@dimjs/logger": "^1.4.8",
    "@dimjs/model": "^1.2.1",
    "@dimjs/model-react": "^1.2.1",
    "@dimjs/utils": "^1.4.4",
    "@wove/react": "^1.2.23",
    "antd": "^5.12.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "use-intl": "3.3.2"
  },
  "devDependencies": {
    "@armit/eslint-config-bases": "^0.1.2",
    "@changesets/cli": "2.27.1",
    "@commitlint/cli": "18.4.3",
    "@commitlint/config-conventional": "18.4.3",
    "@dimjs/request": "^1.4.8",
    "@flatjs/cli": "2.0.2",
    "@flatjs/cli-plugin-mount": "2.0.2",
    "@flatjs/evolve": "2.0.2",
    "@flatjs/mock": "2.0.2",
    "@types/node": "20.10.4",
    "@types/prettier": "2.7.3",
    "@types/react": "18.2.43",
    "@types/react-dom": "18.2.17",
    "@types/shell-quote": "1.7.5",
    "@vitest/coverage-istanbul": "1.0.4",
    "@vitest/ui": "1.0.4",
    "commitizen": "4.3.0",
    "cross-env": "7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.55.0",
    "husky": "8.0.3",
    "is-ci": "3.0.1",
    "lint-staged": "15.2.0",
    "npm-run-all": "4.1.5",
    "picocolors": "1.0.0",
    "postcss": "8.4.32",
    "postcss-preset-env": "9.3.0",
    "prettier": "3.1.1",
    "rimraf": "5.0.5",
    "shell-quote": "1.8.1",
    "tailwind-merge": "^2.1.0",
    "tailwindcss": "3.3.6",
    "ts-node": "10.9.2",
    "typescript": "5.3.3",
    "vite-tsconfig-paths": "4.2.2",
    "vitest": "1.0.4"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": "please-use-yarn",
    "yarn": ">=1.22.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "yarn@4.0.2"
}
